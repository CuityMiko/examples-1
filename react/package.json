{
  "name": "react",
  "version": "1.0.0",
  "description": "routes and restful with react",
  "scripts": {
    "start": "NODE_ENV=production node app.js",
    "dev": "pm2 start app.js --no-daemon --watch=lib --watch=modules --ignore-watch=modules/*/assets/**/*.* --name dev",
    "stop": "pm2 stop dev && pm2 delete dev",
    "build": "rm -rf public/g && plover-build"
  },
  "dependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "bootstrap": "^3.3.7",
    "domready": "^1.0.8",
    "less": "^2.7.2",
    "lodash": "^4.17.4",
    "mz": "^2.6.0",
    "plover-assets-less": "^2.0.0",
    "plover-assets-webpack": "^2.0.0",
    "ploverx": "^3.2.0",
    "pm2": "^2.3.0",
    "query-string": "^4.3.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "webpack": "^2.2.0"
  },
  "license": "ISC",
  "devDependencies": {
    "css-loader": "^0.26.1",
    "node-sass": "^4.3.0",
    "postcss-loader": "^1.2.2",
    "sass-loader": "^4.1.1",
    "scss-loader": "^0.0.1",
    "style-loader": "^0.13.1"
  }
}
