{
  "name": "routes",
  "version": "1.0.0",
  "description": "routes and restful",
  "scripts": {
    "start": "NODE_ENV=production pm2 start app.js --name app",
    "stop": "pm2 stop all && pm2 delete all",
    "dev": "pm2 start app.js --no-daemon --watch --ignore-watch tmp/ --name dev",
    "build": "rm -rf public/g && plover-build"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "global": "^4.3.1",
    "jquery": "^3.1.1",
    "less": "^2.7.2",
    "lodash": "^4.17.4",
    "mz": "^2.6.0",
    "plover-assets-less": "^2.0.0",
    "plover-assets-webpack": "^2.0.0",
    "ploverx": "^3.2.0",
    "pm2": "^2.3.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "webpack": "^2.2.0"
  },
  "license": "ISC"
}
